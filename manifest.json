{
  "name": "new tab & bookmark manager",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "use new tab like desktop first screen",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "storage",
    "unlimitedStorage"
  ]
}
