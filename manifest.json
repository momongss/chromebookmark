{
  "name": "new tab & bookmark manager",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "use new tab like desktop first screen",
  "author": "taehyung",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "assets/bookmark.png"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "./credit/credit.html"
  }
}
